}

.demo1 {
  width: 2rpx;
  height: 2rpx;
  border-radius: 50%;
  background: #fff;
  float: left;
  margin: 0 3px;
  animation: demo1 linear 1s infinite;
  -webkit-animation: demo1 linear 1s infinite;
}

.demo1:nth-child(1) {
  animation-delay: 0s;
}

.demo1:nth-child(2) {
  animation-delay: 0.15s;
}

.demo1:nth-child(3) {
  animation-delay: 0.3s;
}

@keyframes demo1 {
  0%, 60%, 100% {
    transform: scale(1);
  }

  30% {
    transform: scale(2.5);
  }
}

@-webkit-keyframes demo1 {
  0%, 60%, 100% {
    transform: scale(1);
  }

  30% {
    transform: scale(2.5);
  }
}
